{
  "name": "svelte-app",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "vite build",
    "build-dev": "dotenv -e .env.dev vite build",
    "build-prod": "dotenv -e .env.prod vite build",
    "dev": "vite",
    "start": "vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "deploy": "npm run build && firebase deploy --only hosting"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^1.0.0-next.42",
    "@tsconfig/svelte": "^2.0.0",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.1",
    "svelte": "^3.0.0",
    "svelte-check": "^2.0.0",
    "svelte-preprocess": "^4.0.0",
    "tslib": "^2.0.0",
    "typescript": "^4.0.0",
    "vite": "^2.9.6"
  },
  "dependencies": {
    "firebase": "^9.6.8",
    "svelte-spa-router": "^3.2.0"
  }
}
