{
	"name": "routechoice-db",
	"version": "0.0.1",
	"private": true,
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"dev:prod": "vite dev --mode production",
		"dev-offline": "vite dev --mode dev-offline",
		"build": "vite build",
		"build:prod": "npm run migrate:prod && vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"migrations": "drizzle-kit generate",
		"studio": "drizzle-kit studio",
		"migrate": "node scripts/migrate.js",
		"migrate:prod": "node --env-file=.env scripts/migrate-prod.js",
		"drizzle:up": "drizzle-kit up"
	},
	"devDependencies": {
		"@iconify-json/carbon": "^1.1.33",
		"@sveltejs/adapter-cloudflare": "^4.4.0",
		"@sveltejs/enhanced-img": "^0.2.0",
		"@sveltejs/kit": "^2.5.8",
		"@sveltejs/vite-plugin-svelte": "^3.1.0",
		"@unocss/extractor-svelte": "^0.60.2",
		"drizzle-kit": "^0.21.1",
		"prettier": "^3.2.5",
		"prettier-plugin-svelte": "^3.2.3",
		"svelte": "^4.2.17",
		"svelte-check": "^3.4.6",
		"sveltekit-superforms": "^2.13.1",
		"tslib": "^2.6.2",
		"typescript": "^5.4.5",
		"unocss": "^0.60.2",
		"vite": "^5.2.11"
	},
	"dependencies": {
		"@libsql/client": "^0.6.0",
		"@lucia-auth/adapter-drizzle": "^1.0.7",
		"@orienteering-js/gps": "npm:@jsr/orienteering-js__gps@^0.1.10",
		"@orienteering-js/split-times": "npm:@jsr/orienteering-js__split-times@^0.2.0",
		"@picocss/pico": "^2.0.6",
		"drizzle-orm": "^0.30.10",
		"libsql-stateless-easy": "^1.6.8",
		"linkedom": "^0.18.0",
		"lucia": "^3.2.0",
		"ol": "^7.5.2",
		"orienteering-js": "^0.0.29",
		"oslo": "^1.2.0",
		"zod": "^3.23.8"
	}
}
